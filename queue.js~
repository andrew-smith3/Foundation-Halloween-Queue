var Queue = function(){
    this.queue = [];
    this.next = null;
    this.current = null;
    this.done = [];
}

Queue.prototype.dequeue = function(){
    if(this.current != null){
	this.done.push(this.current);
    }
    if(this.next != null){
	this.current = this.next;
    }
    if(this.queue.length != 0){
	this.next = this.queue.shift();
    }
    
}
